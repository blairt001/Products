<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="334.0"
      prefWidth="179.0" stylesheets="@style/drawerCss.css" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="DrawerController">
    <children>
        <ImageView fitHeight="110.0" fitWidth="179.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/back7.jpg"/>
            </image>
        </ImageView>
        <JFXButton fx:id="viewSession" buttonType="RAISED" mnemonicParsing="false" prefHeight="56.0" prefWidth="179.0"
                   onAction="#viewSessionOnAction"
                   styleClass="button"
                   text="View Session"/>
        <JFXButton fx:id="sharedDirectory" buttonType="RAISED" mnemonicParsing="false" prefHeight="56.0"
                   onAction="#sharedDirectoryOnAction"
                   prefWidth="179.0"
                   styleClass="button"
                   text="Show shared directory"/>
        <JFXButton fx:id="changeName" buttonType="RAISED" mnemonicParsing="false" prefHeight="56.0" prefWidth="179.0"
                   onAction="#changeNameOnAction"
                   styleClass="button"
                   text="Change name"/>
        <JFXButton fx:id="logout" buttonType="RAISED" mnemonicParsing="false" prefHeight="56.0" prefWidth="179.0"
                   styleClass="button"
                   text="Logout"/>
    </children>
</VBox>
